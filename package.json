{
  "name": "ide",
  "version": "1.0.9",
  "productName": "UnhandStudio",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Unhand"
  },
  "main": ".webpack/main",
  "homepage": "./",
  "scripts": {
    "icon": "npx iconfont-h5",
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish"
  },
  "config": {
    "husky": {
      "hooks": {
        "commit-msg": "validate-commit-msg"
      }
    },
    "forge": {
      "packagerConfig": {
        "icon": "./public/image/unhand.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "UnhandStudio"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "UnhandStudio",
            "icon": "./public/image/logo.png"
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "unhand-crop",
              "name": "ide"
            },
            "draft": true,
            "prerelease": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "default-src * self blob: data: gap:; style-src * self 'unsafe-inline' blob: data: gap:; script-src * 'self' 'unsafe-eval' 'unsafe-inline' blob: data: gap:; object-src * 'self' blob: data: gap:; img-src * self 'unsafe-inline' blob: data: gap:; connect-src self * 'unsafe-inline' blob: data: gap:; frame-src * self blob: data: gap:;",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/natives/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@dtinsight/molecule": "^0.9.0-beta.4.2",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "@types/dockerode": "^3.3.3",
    "@types/electron-is-dev": "^1.1.1",
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "cz-conventional-changelog": "3.3.0",
    "electron": "17.1.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "husky": "^7.0.4",
    "monaco-editor-webpack-plugin": "6.0.0",
    "node-loader": "^2.0.0",
    "react-iconfont-cli": "^2.0.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "~4.5.4",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@dtinsight/molecule": "^0.9.0-beta.4.2",
    "@mdi/js": "^6.5.95",
    "@mdi/react": "^1.5.0",
    "@microsoft/signalr": "^6.0.2",
    "@types/request": "^2.48.8",
    "@types/unzipper": "^0.10.5",
    "ahooks": "^3.1.13",
    "antd": "^4.18.9",
    "chokidar": "^3.5.3",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "fastify": "^4.0.0-alpha.2",
    "get-port": "^6.1.2",
    "hox": "^1.1.6",
    "lodash": "^4.17.21",
    "nerdctl": "^0.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "rimraf": "^3.0.2",
    "umi-request": "^1.4.0",
    "unzipper": "^0.10.11"
  }
}
